<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:background="#f5f5f5"
    android:layout_height="130dp"
    android:baselineAligned="false">


    <TextView
        android:id="@+id/fechainicio"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="FechaInicio"
        android:layout_alignBottom="@+id/imageView3"
        android:layout_alignStart="@+id/lugar" />

    <TextView
        android:id="@+id/fechatermino"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="FechaTermino"
        android:layout_alignBaseline="@+id/fechainicio"
        android:layout_alignBottom="@+id/fechainicio"
        android:layout_toEndOf="@+id/fechainicio"
        android:layout_marginStart="23dp" />

    <TextView
        android:id="@+id/lugar"
        android:layout_marginTop="25dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Lugar de Viaje"
        android:layout_marginStart="30dp"
        android:layout_alignBaseline="@+id/textView7"
        android:layout_alignBottom="@+id/textView7"
        android:layout_toEndOf="@+id/imageView3" />

    <TextView
        android:id="@+id/total"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="costo"
        android:layout_above="@+id/fechatermino"
        android:layout_toEndOf="@+id/fechatermino"
        android:layout_marginBottom="15dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="$"
        android:layout_alignBaseline="@+id/total"
        android:layout_alignBottom="@+id/total"
        android:layout_toStartOf="@+id/total" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="26dp"
        app:srcCompat="@drawable/viaje"
        android:layout_gravity="center_vertical"
        android:layout_marginTop="50dp"
        android:layout_marginBottom="25dp"
        android:layout_centerVertical="true"
        android:layout_marginLeft="25dp"
        android:layout_alignParentStart="true" />

</RelativeLayout>